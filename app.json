{
  "expo": {
    "name": "CurrencyConverter",
    "description": "Learning React Native + Redux",
    "slug": "CurrencyConverter",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#4F6D7A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bangnguyen.currencyconverter"
    },
    "android": {
      "package": "ccom.bangnguyen.currencyconverter"
    }
  }
}
